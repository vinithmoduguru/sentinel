{
  "name": "backend",
  "version": "1.0.0",
  "type": "module",
  "description": "Minimal TypeScript + Express backend",
  "main": "dist/server.js",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "build": "tsc -p tsconfig.json --outDir dist",
    "start": "node dist/server.js",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write .",
    "prisma:generate": "prisma generate --schema=prisma/schema.prisma",
    "prisma:migrate": "prisma migrate dev --schema=prisma/schema.prisma",
    "prisma:deploy": "prisma migrate deploy --schema=prisma/schema.prisma",
    "prisma:studio": "prisma studio --schema=prisma/schema.prisma",
    "generate:txns": "tsx scripts/generateTransactions.ts",
    "seed": "tsx prisma/seed.ts",
    "seed:fast": "TXN_COUNT=200000 BATCH_SIZE=10000 tsx prisma/seed.ts",
    "seed:1m": "TXN_COUNT=1000000 BATCH_SIZE=20000 tsx prisma/seed.ts",
    "start:with-seed": "npm run prisma:deploy && tsx prisma/seed.ts && npm run start",
    "seed:compose": "npm run prisma:deploy && tsx prisma/seed.ts",
    "eval": "tsx src/scripts/runEvals.ts",
    "test": "echo \"No tests configured\" && exit 0"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@faker-js/faker": "^10.1.0",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "ioredis": "^5.8.2",
    "pino": "^10.1.0",
    "prom-client": "^15.1.3",
    "uuid": "^13.0.0",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@prisma/client": "^6.18.0",
    "@types/express": "^5.0.4",
    "@types/node": "^24.9.2",
    "eslint": "^9.38.0",
    "install": "^0.13.0",
    "nodemon": "^3.1.10",
    "pino-pretty": "^13.1.2",
    "prettier": "^3.6.2",
    "prisma": "^6.18.0",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3"
  }
}
